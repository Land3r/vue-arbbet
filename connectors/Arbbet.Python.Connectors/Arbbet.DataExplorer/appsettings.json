{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "Language": {
    "DefaultCulture": "fr-FR",
    "SupportedCultures": [ "fr-FR", "en-US" ]
  },
  "EmailSender": {
    "Host": "Overwritten",
    "Port": 587,
    "EnableSSL": true,
    "Username": "Overwritten",
    "Password": "Overwritten",
    "From": "Overwritten"
  },
  "Identity": {
    "PasswordRules": {
      "RequiredLength": 4,
      "RequireLowercase": false,
      "RequireUppercase": false,
      "RequireNonAlphanumeric": false,
      "RequireDigit": false
    },
    "LockoutRules": {
      "AllowedForNewUsers": false,
      "DefaultLockoutTimeSpan": "0.01:00:00",
      "MaxFailedAccessAttempts": 10
    },
    "UserRules": {
      "AllowedUserNameCharacters": "",
      "RequireUniqueEmail": true
    },
    "SignInRules": {
      "RequireConfirmedAccount": true,
      "RequireConfirmedEmail": false,
      "RequireConfirmedPhoneNumber": false
    },
    "TwoFactorAuthenticationRules": {
      "Enabled": true,
      "IssuerName": "Arbbet"
    },
    "CookieConfiguration": {
      "Name": "Arbbet",
      "ExpireTimeSpan": "0.01:00:00",
      "SlidingExpiration": true
    },
      "AllowRegistration": true,
      "ConfirmEmail": true
    },
  "AllowedHosts": "*",
  "ConnectionStrings": {
  }
}